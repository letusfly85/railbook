<table border="1">
	<tr>
		<th>ISBNCode</th>
		<th>BookName</th>
		<th>Price</th>
		<th>Publish</th>
		<th>CreateAt</th>
		<th>CD-ROM</th>
	</tr>

<% @books.each do |book| %>
	<tr>
		<td><%= book.isbn %></td>
		<td><%= book.title %></td>
		<td><%= book.price %></td>
		<td><%= book.publish %></td>
		<td><%= book.published %></td>
		<td><%= book.cd %></td>
<% end %>
</table>
